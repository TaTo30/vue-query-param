    "@vue/tsconfig": "^0.5.1",
    "@vuepic/vue-datepicker": "^8.1.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
